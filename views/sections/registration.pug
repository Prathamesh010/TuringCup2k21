				input.nxt(type='button' name='next-btn' value='Next' onclick='nql()')
		i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')
		input.ans-input(style="display: none;")

	.item(style='display: none')
		.question
			span.ques &gt;
			span.ques Enter Transaction ID of Payment:
		.reply
			span.ans $
			input.ans-input(type='text' name='entry.461012999')

		.btns 
			.prev-btn(onclick='pql()')
				i.fa.fa-angle-left(aria-hidden="true" style="color: #76ec7e")
				| &nbsp;
				input.prev(type='button' name='prev-btn' value='Prev')
			.next-btn
				input.nxt(type= 'button' name='next-btn' value='Submit' onclick='submitForm()')

		i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')
