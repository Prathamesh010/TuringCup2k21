.reg.fade-in-top
	span.reg-heading Register<br />
	span.ins Entry fee for participating in Turing Cup 2019 is &#x20B9;50<br />
	span.ins Pay via Cash or UPI as per your option selected in registration form<br />
	span.ins Registrations are closed<br />
	span.ins Receive contest link for Qualifier on 15th February and make your way to the finals<br />
	a(onclick='hideReg()').reg-btn 
		div(onclick='hideReg()') Register Now!<br />
	i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')

form.form-cont.container(action="/form" method="POST" enctype="multipart/form-data" style='display: none')

	.item
		.question
			span.ques &gt;
			span.ques Hello! What&apos;s your Name?
		.reply
			span.ans $
			input.ans-input(type='text' name='name' placeholder='First Last')
		.next-btn()
			input.nxt(type='button' name='next-btn' value='Next' onclick='nql()')
		i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')

	.item(style='display: none')
		.question
			span.ques &gt;
			span.ques PRN/Roll-No :
		.reply
			span.ans $
			input.ans-input(type='text' name='prn' placeholder='117A1028')
		.next-btn
			input.nxt(type='button' name='next-btn' value='Next' onclick='nql()')
		i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')

	.item(style='display: none')
		.question
			span.ques &gt;
			span.ques Email Address
		.reply
			span.ans $
			input.ans-input(type='email' name='email' placeholder='jon@gmail.com')
		.next-btn
			input.nxt(type='button' name='next-btn' value='Next' onclick='nql()')
		i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')

	.item(style='display: none')
		.question
			span.ques &gt;
			span.ques College Name:
		.reply
			span.ans $
			input.ans-input(type='text' name='college' placeholder='SIESGST')
		.next-btn
			input.nxt(type='button' name='next-btn' value='Next' onclick='nql()')
		i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')

	.item(style='display: none')
		.question
			span.ques &gt;
			span.ques Branch:
		.reply
			//- span.ans $
			label.radio-label
				input#ce.input-rad(name='branch' type='radio' value='CE' checked='checked')
				span.inner-label CE
			label.radio-label
				input#it.input-rad(name='branch' value='IT' type='radio')
				span.inner-label IT
			label.radio-label
				input#extc.input-rad(name='branch' value='EXTC' type='radio')
				span.inner-label EXTC
			label.radio-label
				input#oth.input-rad(name='branch' value='Other' type='radio')
				span.inner-label Other

		.next-btn
			input.nxt(type='button' name='next-btn' value='Next' onclick='nql()')
		i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')

	.item(style='display: none')
		.question
			span.ques &gt;
			span.ques Year:
		.reply
			//- span.ans $
			//- input.ans-input(type='text' name='entry.588393791' placeholder='FE')
			label.radio-label
				input#fe.input-rad(name='year' type='radio' value='FE' checked='checked')
				span.inner-label FE
			label.radio-label
				input#se.input-rad(name='year' value='SE' type='radio')
				span.inner-label SE
			label.radio-label
				input#te.input-rad(name='year' value='TE' type='radio')
				span.inner-label TE
			label.radio-label
				input#be.input-rad(name='year' value='BE' type='radio')
				span.inner-label BE
		.next-btn
			input.nxt(type='button' name='next-btn' value='Next' onclick='nql()')
		i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')

	.item(style='display: none')
		.question
			span.ques &gt;
			span.ques Upload ScreenShot of Payment:
		.reply
			span.ans $
			input.ans-input(type='file' name='upload' )

		.next-btn
			input.nxt(type='submit' name='next-btn' value='Submit')
		i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')
script(src="./js/regJs.js")
