			.prev-btn(onclick='pql()')
				i.fa.fa-angle-left(aria-hidden="true" style="color: #76ec7e")
				| &nbsp;
				input.prev(type='button' name='prev-btn' value='Prev')
			.next-btn
				input.nxt(type='button' name='next-btn' value='Next' onclick='nql()')
		i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')

	.item(style='display: none')
		.question
			span.ques &gt;
			span.ques Branch:
		.reply
			//- span.ans $
			label.radio-label
				input#ce.input-rad(name='entry.196002535' type='radio' value='CE' checked='checked')
				span.inner-label CE
			label.radio-label
				input#it.input-rad(name='entry.196002535' value='IT' type='radio')
				span.inner-label IT
			label.radio-label
				input#extc.input-rad(name='entry.196002535' value='EXTC' type='radio')
				span.inner-label EXTC
			label.radio-label
				input#oth.input-rad(name='entry.196002535' value='Other' type='radio')
				span.inner-label Other

		.btns 
			.prev-btn(onclick='pql()')
				i.fa.fa-angle-left(aria-hidden="true" style="color: #76ec7e")
				| &nbsp;
				input.prev(type='button' name='prev-btn' value='Prev')
			.next-btn
				input.nxt(type='button' name='next-btn' value='Next' onclick='nql()')
		i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')
		input.ans-input(style="display: none;")

	.item(style='display: none')
		.question
			span.ques &gt;
			span.ques Year:
