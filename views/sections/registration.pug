.reg.fade-in-top
	span.reg-heading Register<br />
	span.ins Entry fee for participating in Turing Cup 2019 is &#x20B9;50<br />
	span.ins Pay via Cash or UPI as per your option selected in registration form<br />
	span.ins Registrations are closed<br />
	span.ins Receive contest link for Qualifier on 15th February and make your way to the finals<br />
	a.reg-btn(onclick='hideReg()') Register Now!<br />
	i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')

form.form-cont.container(action='https://docs.google.com/forms/d/e/1FAIpQLSe3w7tJepz8AyCNHmhxhTQQoqSwC32hwTWYerVdgSixbbJS9w/formResponse' style='display: none')
	
	.item
		.question
			span.ques &gt;
			span.ques Hello! What&apos;s your Name?
		.reply
			span.ans $
			input.ans-input(type='text' name='entry.2092238618' placeholder='First Last')
		.next-btn
			input.nxt(type='button' name='next-btn' value='Next' onclick='nql()')
		i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')

	.item(style='display: none')
		.question
			span.ques &gt;
			span.ques Email Address
		.reply
			span.ans $
			input.ans-input(type='email' name='entry.1556369182' placeholder='jon@gmail.com')
		.next-btn
			input.nxt(type='button' name='next-btn' value='Next' onclick='nql()')
		i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')

	.item(style='display: none')
		.question
			span.ques &gt;
			span.ques College Name:
		.reply
			span.ans $
			input.ans-input(type='text' name='entry.479301265' placeholder='SIESGST')
		.next-btn
			input.nxt(type='button' name='next-btn' value='Next' onclick='nql()')
		i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')

	.item(style='display: none')
		.question
			span.ques &gt;
			span.ques Branch:
		.reply
			//- span.ans $
			label.radio-label
				input#ce.input-rad(name='entry.1091102377' type='radio' value='CE' checked='checked')
				span.inner-label CE
			label.radio-label
				input#it.input-rad(name='entry.1091102377' value='IT' type='radio')
				span.inner-label IT
			label.radio-label
				input#extc.input-rad(name='entry.1091102377' value='EXTC' type='radio')
				span.inner-label EXTC
			label.radio-label
				input#oth.input-rad(name='entry.1091102377' value='Other' type='radio')
				span.inner-label Other

		.next-btn
			input.nxt(type='button' name='next-btn' value='Next' onclick='nql()')
		i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')

	.item(style='display: none')
		.question
			span.ques &gt;
			span.ques Year:
		.reply
			//- span.ans $
			//- input.ans-input(type='text' name='entry.588393791' placeholder='FE')
			label.radio-label
				input#fe.input-rad(name='entry.588393791' type='radio' value='FE' checked='checked')
				span.inner-label FE
			label.radio-label
				input#se.input-rad(name='entry.588393791' value='SE' type='radio')
				span.inner-label SE
			label.radio-label
				input#te.input-rad(name='entry.588393791' value='TE' type='radio')
				span.inner-label TE
			label.radio-label
				input#be.input-rad(name='entry.588393791' value='BE' type='radio')
				span.inner-label BE
		.next-btn
			input.nxt(type='submit' name='next-btn' value='Submit')
		i.fa.fa-2x.fa-chevron-down.arrow(aria-hidden='true')

script.

	var x = document.getElementsByClassName("reg")[0]
	var y = document.getElementsByClassName("item")
	var z = document.getElementsByClassName("form-cont")[0]

	function hideReg() {
		x.style.display = "none"
		z.style.display = "flex"
		y[0].className = "item fade-in-bottom"
	}

	var count = 0
	function nql() {
		count++
		console.log(y[count - 1])
		y[count - 1].className = "item fade-out-up"
		y[count - 1].style.display = "none"
		y[count].style.display = "flex"
		y[count].className = "item fade-in-bottom"
	}

	function pql() {
		count--
		y[count + 1].className = "item fade-out-bottom"
		y[count + 1].style.display = "none"
		y[count].style.display = "flex"
		y[count].className = "item fade-in-top"
	}